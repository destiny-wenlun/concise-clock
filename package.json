{
  "name": "concise-clock",
  "version": "1.0.1",
  "description": "一个基于h5 canvas的模拟时钟javascript程序",
  "main": "lib/clock.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "tsc -watch",
    "es": "tsc --lib dom,es2015 --strict false --target es2017  src/clock.ts",
    "release": "tsc --lib dom,es2015 --strict false --target es5 --declaration true --declarationDir release/lib --outDir release/lib src/clock.ts",
    "uglify": "uglifyjs dist/clock.js -o dist/clock.min.js",
    "build": "webpack & npm run uglify"
  },
  "keywords": [
    "js",
    "h5",
    "canvas",
    "clockwise",
    "clock",
    "Analog Clock",
    "模拟时钟"
  ],
  "author": "wenlun",
  "license": "ISC",
  "devDependencies": {
    "ts-loader": "^3.1.1",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9",
    "webpack": "^3.3.0"
  }
}